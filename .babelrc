{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ]
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "@routes": "./src/routes",
                    "@controllers": "./src/controllers",
                    "@middlewares": "./src/middlewares",
                    "@core": "./src/core",
                    "@models": "./src/models",
                    "@config": "./src/config",
                    "@services": "./src/services"
                }
            }
        ]
    ],
    "sourceMaps": "inline",
    "retainLines": true
}